  object FDQueryRelatorio: TFDQuery
    Connection = DataModule1.FDConnection
    SQL.Strings = (
      'SELECT DATA, TANQUE, BOMBAS, PRECO '
      'FROM '
      '('
      'SELECT TB_VENDATC1.DATA AS DATA,  '
      '       CASE TB_VENDATC1.BOMBA_GASOL'
      '        WHEN 1 THEN '#39'Tanque de Gasolina'#39
      '        WHEN 2 THEN '#39'Tanque de Gasolina'#39
      '       ELSE '#39'Outras Bombas'#39' END AS TANQUE,'
      '       CASE TB_VENDATC1.BOMBA_GASOL'
      '        WHEN 1 THEN '#39'Bomba de Gasolina 1'#39
      '        WHEN 2 THEN '#39'Bomba de Gasolina 2'#39
      '       ELSE '#39'Outras Bombas'#39' END AS BOMBAS,'
      '       TB_VENDATC1.PRECO_VENDA as PRECO'
      'FROM TB_VENDATC1'
      
        'WHERE (TB_VENDATC1.DATA >= :datainicial AND TB_VENDATC1.DATA <= ' +
        ':datafinal)'
      ''
      ''
      'UNION'
      ''
      ''
      'SELECT '
      '       TB_VENDATC2.DATA AS DATA,'
      '       CASE TB_VENDATC2.BOMBA_OLEO'
      '        WHEN 1 THEN '#39'Tanque de Oleo'#39
      '        WHEN 2 THEN '#39'Tanque de Oleo'#39
      '       ELSE '#39'Outras Bombas'#39' END AS TANQUE,'
      '       CASE TB_VENDATC2.BOMBA_OLEO'
      '        WHEN 1 THEN '#39'Bomba de Oleo 1'#39
      '        WHEN 2 THEN '#39'Bomba de Oleo 2'#39
      '       ELSE '#39'Outras Bombas'#39' END AS BOMBAS, '
      '       TB_VENDATC2.PRECO_VENDA AS PRECO'
      'FROM   TB_VENDATC2'
      
        'WHERE (TB_VENDATC2.DATA >= :datainicial AND TB_VENDATC2.DATA <= ' +
        ':datafinal)'
      ')'
      'GROUP BY DATA, TANQUE, BOMBAS, PRECO')
    Left = 568
    Top = 8
    ParamData = <
      item
        Name = 'DATAINICIAL'
        DataType = ftDate
        ParamType = ptInput
        Value = 42528d
      end
      item
        Name = 'DATAFINAL'
        DataType = ftDate
        ParamType = ptInput
        Value = 42528d
      end>
